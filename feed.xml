&lt;p align=&quot;right&quot; class=&quot;p&quot; style=&quot;text-indent:32.0000pt; text-align:right&quot;&gt;&lt;span style=&quot;layout-grid-mode:char&quot;&gt;&lt;span style=&quot;word-break:break-all&quot;&gt;&lt;span style=&quot;text-autospace:ideograph-numeric&quot;&gt;&lt;font face=&quot;方正仿宋简体&quot;&gt;航天&lt;/font&gt;&lt;font face=&quot;方正仿宋简体&quot;&gt;学院&lt;/font&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;

&lt;p align=&quot;right&quot; class=&quot;p&quot; style=&quot;text-indent:32.0000pt; text-align:right&quot;&gt;&lt;span style=&quot;layout-grid-mode:char&quot;&gt;&lt;span style=&quot;word-break:break-all&quot;&gt;&lt;span style=&quot;text-autospace:ideograph-numeric&quot;&gt;&lt;font face=&quot;方正仿宋简体&quot;&gt;电信学院&lt;/font&gt; &amp;nbsp;&amp;nbsp;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;

&lt;p align=&quot;right&quot; class=&quot;p&quot; style=&quot;text-indent:32.0000pt; text-align:right&quot;&gt;&lt;span style=&quot;layout-grid-mode:char&quot;&gt;&lt;span style=&quot;text-autospace:ideograph-numeric&quot;&gt;&lt;font face=&quot;Times New Roman&quot;&gt;2025&lt;/font&gt;&lt;font face=&quot;方正仿宋简体&quot;&gt;年&lt;/font&gt;&lt;font face=&quot;Times New Roman&quot;&gt;6&lt;/font&gt;&lt;font face=&quot;方正仿宋简体&quot;&gt;月&lt;/font&gt;&lt;font face=&quot;Times New Roman&quot;&gt;26&lt;/font&gt;&lt;font face=&quot;方正仿宋简体&quot;&gt;日&lt;/font&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;</content:encoded>
