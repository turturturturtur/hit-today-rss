			&lt;p align=&quot;center&quot; style=&quot;text-align:center&quot;&gt;课程和师资&lt;/p&gt;
			&lt;/td&gt;
		&lt;/tr&gt;
		&lt;tr&gt;
			&lt;td rowspan=&quot;3&quot; style=&quot;border-width: medium 1px 1px; border-style: none solid solid; border-color: currentcolor black black; padding: 0cm 7px; height: 53px;&quot; valign=&quot;top&quot;&gt;
			&lt;p align=&quot;center&quot; style=&quot;text-align:center&quot;&gt;5&lt;/p&gt;

			&lt;p align=&quot;center&quot; style=&quot;text-align:center&quot;&gt;月&lt;/p&gt;

			&lt;p align=&quot;center&quot; style=&quot;text-align:center&quot;&gt;22&lt;/p&gt;

			&lt;p align=&quot;center&quot; style=&quot;text-align:center&quot;&gt;日&lt;/p&gt;
			&lt;/td&gt;
			&lt;td style=&quot;border-width: medium 1px 1px medium; border-style: none solid solid none; border-color: currentcolor black black currentcolor; padding: 0cm 7px; height: 53px;&quot; valign=&quot;top&quot;&gt;
			&lt;p style=&quot;text-align:justify&quot;&gt;8：30-9：00&lt;/p&gt;
			&lt;/td&gt;
			&lt;td style=&quot;border-width: medium 1px 1px medium; border-style: none solid solid none; border-color: currentcolor black black currentcolor; padding: 0cm 7px; height: 53px;&quot; valign=&quot;top&quot;&gt;
