&lt;p align=&quot;justify&quot; class=&quot;p&quot; style=&quot;text-indent:32.0000pt; text-align:justify&quot;&gt;&amp;nbsp;&lt;/p&gt;

&lt;p align=&quot;right&quot; class=&quot;p&quot; style=&quot;text-indent:32.0000pt; text-align:right&quot;&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;span style=&quot;word-break:break-all&quot;&gt;&lt;span style=&quot;text-autospace:ideograph-numeric&quot;&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;span style=&quot;letter-spacing:0.0000pt&quot;&gt;&lt;span style=&quot;font-style:normal&quot;&gt;&lt;font face=&quot;仿宋&quot;&gt;共青团哈尔滨工业大学土木工程学院委员会&lt;/font&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;

&lt;p align=&quot;right&quot; class=&quot;p&quot; style=&quot;text-indent:32.0000pt; text-align:right&quot;&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;span style=&quot;word-break:break-all&quot;&gt;&lt;span style=&quot;text-autospace:ideograph-numeric&quot;&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;span style=&quot;letter-spacing:0.0000pt&quot;&gt;&lt;span style=&quot;font-style:normal&quot;&gt;&lt;font face=&quot;仿宋&quot;&gt;&lt;span style=&quot;font-family:Times New Roman,Times,serif;&quot;&gt;2025&lt;/span&gt;年&lt;span style=&quot;font-family:Times New Roman,Times,serif;&quot;&gt;6&lt;/span&gt;月&lt;/font&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;span style=&quot;font-family:Times New Roman,Times,serif;&quot;&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;span style=&quot;letter-spacing:0.0000pt&quot;&gt;&lt;span style=&quot;font-style:normal&quot;&gt;9&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;span style=&quot;letter-spacing:0.0000pt&quot;&gt;&lt;span style=&quot;font-style:normal&quot;&gt;&lt;font face=&quot;仿宋&quot;&gt;日&amp;nbsp;&amp;nbsp;&lt;/font&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;</content:encoded>
