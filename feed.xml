            <pubDate>Mon, 09 Jun 2025 06:22:20 GMT</pubDate>
            <content:encoded>&lt;p class=&quot;p&quot; style=&quot;margin-top:14px; margin-bottom:14px; text-indent:24.0000pt; text-align:left&quot;&gt;&lt;span style=&quot;word-break:break-all&quot;&gt;&lt;span style=&quot;text-autospace:ideograph-numeric&quot;&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;font face=&quot;仿宋&quot;&gt;为引导学生&lt;/font&gt;&lt;/span&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;font face=&quot;仿宋&quot;&gt;积极健康成长&lt;/font&gt;&lt;/span&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;font face=&quot;仿宋&quot;&gt;，全面提升学生的职业规划能力，助力学生更好地认识自我、管理情绪，有效应对学业、生活上的挑战，明确个人兴趣与优势，科学规划职业生涯，&lt;/font&gt;&lt;/span&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;font face=&quot;仿宋&quot;&gt;商&lt;/font&gt;&lt;/span&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;font face=&quot;仿宋&quot;&gt;学院于&lt;/font&gt;&lt;font face=&quot;仿宋&quot;&gt;202&lt;/font&gt;&lt;/span&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;font face=&quot;仿宋&quot;&gt;5&lt;/font&gt;&lt;/span&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;font face=&quot;仿宋&quot;&gt;年&lt;/font&gt;&lt;/span&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;font face=&quot;仿宋&quot;&gt;春&lt;/font&gt;&lt;/span&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;font face=&quot;仿宋&quot;&gt;季学期启动&lt;/font&gt;&lt;/span&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;font face=&quot;仿宋&quot;&gt;“&lt;/font&gt;&lt;/span&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;font face=&quot;仿宋&quot;&gt;商悦心语&lt;/font&gt;&lt;/span&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;font face=&quot;仿宋&quot;&gt;”&lt;/font&gt;&lt;/span&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;font face=&quot;仿宋&quot;&gt;二级心理辅导站&lt;/font&gt;&lt;/span&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;font face=&quot;仿宋&quot;&gt;和就业咨询室&lt;/font&gt;&lt;/span&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;font face=&quot;仿宋&quot;&gt;，&lt;/font&gt;&lt;/span&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;font face=&quot;仿宋&quot;&gt;现将&lt;/font&gt;&lt;/span&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;font face=&quot;仿宋&quot;&gt;第&lt;/font&gt;&lt;/span&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;font face=&quot;仿宋&quot;&gt;十六&lt;/font&gt;&lt;/span&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;font face=&quot;仿宋&quot;&gt;周咨询安排公布如下：&lt;/font&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;
&lt;img alt=&quot;&quot; data-entity-type=&quot;file&quot; data-entity-uuid=&quot;d7242da0-1fe1-40ee-8139-f0155a840a3e&quot; src=&quot;/sites/today1.prod1.dpweb1.hit.edu.cn/files/inline-images/2025/06/09/1.png&quot; width=&quot;600&quot; class=&quot;align-center&quot;&gt;
&lt;p class=&quot;p&quot; style=&quot;margin-top:14px; margin-bottom:14px; text-indent:24.0000pt; text-align:left&quot;&gt;&lt;span style=&quot;word-break:break-all&quot;&gt;&lt;span style=&quot;text-autospace:ideograph-numeric&quot;&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;font face=&quot;仿宋&quot;&gt;【面向对象】&lt;/font&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;

&lt;p class=&quot;p&quot; style=&quot;margin-top:14px; margin-bottom:14px; text-indent:24.0000pt; text-align:left&quot;&gt;&lt;span style=&quot;word-break:break-all&quot;&gt;&lt;span style=&quot;text-autospace:ideograph-numeric&quot;&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;font face=&quot;仿宋&quot;&gt;商&lt;/font&gt;&lt;/span&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;font face=&quot;仿宋&quot;&gt;学院全体学生&lt;/font&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;

&lt;p class=&quot;p&quot; style=&quot;margin-top:14px; margin-bottom:14px; text-indent:24.0000pt; text-align:left&quot;&gt;&lt;span style=&quot;word-break:break-all&quot;&gt;&lt;span style=&quot;text-autospace:ideograph-numeric&quot;&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;font face=&quot;仿宋&quot;&gt;【预约方式】&lt;/font&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;

&lt;p class=&quot;p&quot; style=&quot;margin-top:14px; margin-bottom:14px; text-indent:24.0000pt; text-align:left&quot;&gt;&lt;span style=&quot;word-break:break-all&quot;&gt;&lt;span style=&quot;text-autospace:ideograph-numeric&quot;&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;font face=&quot;仿宋&quot;&gt;咨询辅导采取学生预约的方式，学生可通过提交问卷进行预约。&lt;/font&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;

&lt;p class=&quot;p&quot; style=&quot;margin-top:14px; margin-bottom:14px; text-indent:24.0000pt; text-align:left&quot;&gt;&lt;span style=&quot;word-break:break-all&quot;&gt;&lt;span style=&quot;text-autospace:ideograph-numeric&quot;&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;font face=&quot;仿宋&quot;&gt;预约时可选取&lt;/font&gt;&lt;font face=&quot;仿宋&quot;&gt;“一对一”咨询或同一咨询方向的团体报名。&lt;/font&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;

&lt;p class=&quot;p&quot; style=&quot;margin-top:14px; margin-bottom:14px; text-indent:24.0000pt; text-align:left&quot;&gt;&lt;span style=&quot;word-break:break-all&quot;&gt;&lt;span style=&quot;text-autospace:ideograph-numeric&quot;&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;font face=&quot;仿宋&quot;&gt;如需借用咨询室进行面试，请提前联系年级辅导员老师。&lt;/font&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;

&lt;p class=&quot;p&quot; style=&quot;margin-top:14px; margin-bottom:14px; text-indent:24.0000pt; text-align:left&quot;&gt;&lt;span style=&quot;word-break:break-all&quot;&gt;&lt;span style=&quot;text-autospace:ideograph-numeric&quot;&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;font face=&quot;仿宋&quot;&gt;【时间安排】&lt;/font&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;

&lt;p class=&quot;p&quot; style=&quot;margin-top:14px; margin-bottom:14px; text-indent:24.0000pt; text-align:left&quot;&gt;&lt;span style=&quot;word-break:break-all&quot;&gt;&lt;span style=&quot;text-autospace:ideograph-numeric&quot;&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;font face=&quot;仿宋&quot;&gt;学生预约咨询开放时间为每周&lt;/font&gt;&lt;/span&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;font face=&quot;仿宋&quot;&gt;一&lt;/font&gt;&lt;/span&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;font face=&quot;仿宋&quot;&gt;至周五。&lt;/font&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;

&lt;p class=&quot;p&quot; style=&quot;margin-top:14px; margin-bottom:14px; text-indent:24.0000pt; text-align:left&quot;&gt;&lt;span style=&quot;word-break:break-all&quot;&gt;&lt;span style=&quot;text-autospace:ideograph-numeric&quot;&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;font face=&quot;仿宋&quot;&gt;“一对一”咨询，时长不限，建议以1小时为上限；&lt;/font&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;

&lt;p class=&quot;p&quot; style=&quot;margin-top:14px; margin-bottom:14px; text-indent:24.0000pt; text-align:left&quot;&gt;&lt;span style=&quot;word-break:break-all&quot;&gt;&lt;span style=&quot;text-autospace:ideograph-numeric&quot;&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;font face=&quot;仿宋&quot;&gt;“一对多”咨询辅导，时长不限，建议以1.5小时为上限；&lt;/font&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;

&lt;p class=&quot;p&quot; style=&quot;margin-top:14px; margin-bottom:14px; text-indent:24.0000pt; text-align:left&quot;&gt;&lt;span style=&quot;word-break:break-all&quot;&gt;&lt;span style=&quot;text-autospace:ideograph-numeric&quot;&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;font face=&quot;仿宋&quot;&gt;【开放地点】&lt;/font&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;

&lt;p class=&quot;p&quot; style=&quot;margin-top:14px; margin-bottom:14px; text-indent:24.0000pt; text-align:left&quot;&gt;&lt;span style=&quot;word-break:break-all&quot;&gt;&lt;span style=&quot;text-autospace:ideograph-numeric&quot;&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;font face=&quot;仿宋&quot;&gt;“&lt;/font&gt;&lt;/span&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;font face=&quot;仿宋&quot;&gt;商悦心语&lt;/font&gt;&lt;/span&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;font face=&quot;仿宋&quot;&gt;”二级心理辅导站&lt;/font&gt;&lt;/span&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;font face=&quot;仿宋&quot;&gt;、&lt;/font&gt;&lt;/span&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;font face=&quot;仿宋&quot;&gt;就业咨询室&lt;/font&gt;&lt;/span&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;font face=&quot;仿宋&quot;&gt;——经管学院楼20&lt;/font&gt;&lt;/span&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;font face=&quot;仿宋&quot;&gt;6&lt;/font&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;

&lt;p class=&quot;p&quot; style=&quot;margin-top:14px; margin-bottom:14px; text-indent:24.0000pt; text-align:left&quot;&gt;&lt;span style=&quot;word-break:break-all&quot;&gt;&lt;span style=&quot;text-autospace:ideograph-numeric&quot;&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;font face=&quot;仿宋&quot;&gt;【预约流程】&lt;/font&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;

&lt;p class=&quot;p&quot; style=&quot;margin-top:14px; margin-bottom:14px; text-indent:24.0000pt; text-align:left&quot;&gt;&lt;span style=&quot;word-break:break-all&quot;&gt;&lt;span style=&quot;text-autospace:ideograph-numeric&quot;&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;font face=&quot;仿宋&quot;&gt;点击下方链接进行报名，工作组提前一天将报名信息反馈至预约老师。&lt;/font&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;

&lt;p style=&quot;text-indent:22.0000pt; text-align:justify&quot;&gt;&lt;span style=&quot;text-autospace:ideograph-numeric&quot;&gt;&lt;span style=&quot;background:#ffffff&quot;&gt;&lt;span style=&quot;letter-spacing:0.0000pt&quot;&gt;&lt;span style=&quot;font-style:normal&quot;&gt;&lt;font face=&quot;宋体&quot;&gt;https://www.wjx.cn/vm/QiVW9hK.aspx# &lt;/font&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;</content:encoded>
